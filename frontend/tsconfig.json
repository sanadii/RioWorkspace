{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "baseUrl": "./src", // This must be specified if "paths" is.
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "syncfusion/*": ["syncfusion/*"],
      "helpers/*": ["helpers/*"],
      "assets/*": ["assets/*"],
      "Components/*": ["Components/*"],
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/config.js", "src/Components/Common/Table/TableContainer.tsx", "src/Components/Common/Table/TableColumns.xxx", "src/pages/DailyRevenues/useRecentDailyRevenues.js", "src/pages/Calendar/index.tsx"],
  "exclude": ["node_modules"]
}

{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,

    // was true
    "strict": false,
    "strictNullChecks": false, // Turn off strict null checks only

    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "baseUrl": "./src",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "syncfusion/*": ["syncfusion/*"],
      "interfaces/*": ["interfaces/*"],
      "helpers/*": ["helpers/*"],
      "assets/*": ["assets/*"],
      "shared/components/*": ["shared/components*"],
      "shared/hooks/*": ["shared/hooks/*"],
      "shared/constants/*": ["shared/constants/*"],
      "Components/*": ["Components/*"],
      "types/*": ["types/*"],
      "@/*": ["./*"]
    }
  },
  "include": ["src/**/*", "**/*.ts", "**/*.tsx", "src/pages/Invoice/remove", "src/pages/Scheduler/.trash/tests-addAppointment.js", "src/pages/Scheduler/.trash/LiveSearch.tsx", "src/pages/Scheduler/.trash/LiveSearch copy.tsx", "src/pages/Scheduler/.trash/index-bk.tsx", "src/pages/Scheduler/.trash/index copy 2.tsx", "src/pages/Scheduler/.trash/index copy 3.tsx", "src/pages/Scheduler/.trash/index copy.tsx"],
  "exclude": ["node_modules"]
}
